Algoritmo CON09
	Definir N1, N2, N3 Como Entero
	Escribir 'INTRODUSCA EL PRIMER NUM '
	Leer N1
	Escribir 'INTRODUSCA EN 2DO NUMENO'
	Leer N2
	Si N1>N2 Entonces
		T <- N1
		N1 <- N2
		N2 <- T
		Escribir 'NUMERO INTERCANMBIADOS '
		Escribir 'NUMERO 1: ', N1
		Escribir 'NUMERO 2; ', N2
	SiNo
		Escribir 'NUMEROS SIN INTERCAMBIAR'
		Escribir 'NUMERO 1: ', N1
		Escribir 'NUMERO 2: ', N2
	FinSi
FinAlgoritmo
