Algoritmo CON01
	Definir NUM Como Entero
	Escribir 'INTODUSCA NUMERO ENTERO'
	Leer NUM
	Si NUM>0 Entonces
		Escribir 'EL NUMERO ES POSITIVO '
	SiNo
		Si NUM=0 Entonces
			Escribir 'EL NUMERO NO TIENE SIGNO'
		SiNo
			Escribir 'EL NUERO ES NEGARIVO '
		FinSi
	FinSi
FinAlgoritmo
