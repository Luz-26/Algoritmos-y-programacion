Algoritmo CON05
	Definir NUM Como Real
	Escribir 'PORFAVOR DIGITE UN NUMERO'
	Leer NUM
	Si NUM<100 Entonces
		Escribir 'EL NUMERO ES MENOR QUE 100'
	SiNo
		Si NUM>100 Entonces
			Escribir 'EL NUMERO ES MAYOR QUE 100'
		SiNo
			Si NUM=100 Entonces
				Escribir 'ESTE NUMERO ES 100'
			FinSi
		FinSi
	FinSi
FinAlgoritmo
