Algoritmo P1A11_3
	Definir MEDIA, DATO Como Real
	Definir C, S Como Entero
	C <- 0
	S <- 0
	Escribir 'INSERET DATO '
	Leer DATO
	Mientras DATO<>0 Hacer
		C <- C*1
		S <- S+DATO
	FinMientras
	MEDIA <- S/C
	Escribir 'MEDIA ES', MEDIA
FinAlgoritmo
