Algoritmo SEC03
	Definir A, B Como Entero
	Definir s, R, D, M Como Entero
	Escribir ' introdusca un número entero: '
	Leer A
	Escribir ' introdusca otro numero entero '
	Leer B
	s <- A+B
	R <- A-B
	M <- A*B
	D <- trunc(A/B)
	Escribir ' la suma es:', s, ' La resta es:', R, ' La multiplicacion es:', M, ' La divicion es:', D
FinAlgoritmo
