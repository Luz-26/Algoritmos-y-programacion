Algoritmo P1A13_13
	Definir CONT Como Entero
	Definir NUM Como Real
	CONT<-0
	Repetir
		Escribir "INGRESE UNA SERIE DE NUM , LA SERIE TERMINARA CUANDO DIGITE EL 0"
		Leer NUM
		CONT<-CONT+1
	Hasta Que NUM=0
	CONT<-CONT-1
	Escribir "EL NUMERO DE NUMEROS EN LA SUCESION ES ", CONT
FinAlgoritmo
