Algoritmo P1A13_11
	Definir N1,N2 Como Entero
	Escribir "DIGITE DOS NUMEROS"
	Leer N1
	Leer N2
	
	Escribir "LA SUMA DE LOS DOS NUMEROS ES: ", N1+N2
	Escribir "LA RESTA DE LOS DOS NUMEROS ES: ", N1-N2
	Escribir " LA MULTIPLICACION DE LOS NUMEROS ES: ", N1*N2
	SI N2<>0 Entonces
		Escribir "LA DIVICION DE LOS DOS NUMEROS ES:  ", N1/N2
		SINO 
			Escribir " NO SE PUEDE HACE LA DIVICION "
		FinSi
	
	
FinAlgoritmo
