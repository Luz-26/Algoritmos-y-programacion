Algoritmo P1A13_7
	Definir CONT Como Entero
	Definir NUM,SUM,MEDIA Como Real
	CONT<-0
	Repetir
		CONT<-CONT+1
		Escribir "DIGITE UN NUM"
		Leer NUM
		SUM<-SUM+NUM
		MEDIA<-SUM/CONT
	Hasta Que CONT=5
	Escribir "LA MEDIA ES ",MEDIA
	
FinAlgoritmo
